<Application x:Class="SimpleAudio.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SimpleAudio"
             xmlns:convert="clr-namespace:SimpleAudio.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <convert:RelativeWidthConverter x:Key="RelativeWidthConverter" />
            <convert:StatusIconConverter x:Key="StatusIconConverter" />
            <convert:StatusTaskbarIconConverter x:Key="StatusTaskbarIconConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
